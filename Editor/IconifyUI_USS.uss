#TextureBox > #Select > * > .unity-object-field__selector {
    width: 100%;
    height: 100%;
}

#FolderOptions > * > .unity-list-view__item:hover {
    background-color: rgba(255, 255, 255, 0.05);
}

#FolderOptions > * > .unity-list-view__item:checked {
    background-color: rgba(0, 104, 255, 0.26);
}

.reaorderable_dot {
    background-color: rgba(255, 255, 255, 0.73);
    width: 14px;
    height: 1px;
    flex-shrink: 0;
    flex-grow: 0;
    margin-top: 4px;
}
